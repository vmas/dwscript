<script>

function GetMyOne() {
   return {
      Field : "hello",
      MyMethod : function (s) { return this.Field+" "+s; }
   };
}

function Print(s) { document.write(s) };
function PrintLn(s) { Print(s); document.writeln("<br />") };

<%pas2js
type
   TExternalClass = class external
      Field : String;
      function Method(s : String) : String; name "MyMethod";
   end;

function GetOne : TExternalClass; external; name "GetMyOne";

var obj := GetOne;

PrintLn(obj.Field);
PrintLn(obj.Method('World'));

obj.Field:='bye bye';
PrintLn(obj.Method('World'));

%>

</script>
